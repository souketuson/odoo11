<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <act_window id="action_purchase_wizard"
                name="Purchase wizard"
                src_model="yc.purchase"
                res_model="purchase.wizard"
                view_mode="form"
                target="new"
                multi="True"/>

    <record model="ir.ui.view" id="purchase_wizard">
        <field name="name">Purchase Report</field>
        <field name="model">purchase.wizard</field>
        <!--<field name="type">form</field>-->
        <field name="arch" type="xml">
            <form string="Purchase Report">
                <group>
                    <fields name="purchase_ids" widget="many2many_list"/>
                </group>
                <footer>
                    <button name="" string="comfirm" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


